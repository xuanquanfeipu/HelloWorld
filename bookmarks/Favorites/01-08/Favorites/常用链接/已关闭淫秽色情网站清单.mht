        }

        document.all(sName).style.top =3D nTop;
        document.all(sName).style.left =3D nLeft;
    }

    setTimeout("adMoveRefresh(\"" + sName +"\"," + nStyle + ");",500);
}

function adMove(sRet, sName, nStyle)
{
